	<dc:title xml:lang="en">A Tissue culture-derived Pasteurella haemolytica vaccine</dc:title>
	<dc:creator xml:lang="en">Loan, R. W.</dc:creator>
	<dc:creator xml:lang="en">Tigges, M. G.</dc:creator>
	<dc:creator xml:lang="en">Purdy, C. W.</dc:creator>
